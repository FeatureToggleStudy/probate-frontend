{
  "name": "probate-frontend",
  "description": "Probate NodeJS web app",
  "version": "4.1.2",
  "private": true,
  "engines": {
    "node": ">=8.9.4"
  },
  "scripts": {
    "setup": "set $NODE_PATH=. && npm run sass && npm run git-info && npm run sass-ie8",
    "start": "set $NODE_PATH=. && node server.js",
    "debug": "set $NODE_PATH=. && node inspect server.js",
    "prestart:dev": "npm run sass:watch",
    "start:dev": "set $NODE_PATH=. && nodemon -e js,json --watch 'app/' --watch 'test/' -x 'node server.js'",
    "sass:watch": "set $NODE_PATH=. && watch 'npm run -s sass' app/assets/sass/ &",
    "sass": "set $NODE_PATH=. && node-sass app/assets/sass/application.scss --output public/stylesheets --quiet --output-style expanded --source-map true --include-path node_modules/govuk_frontend",
    "sass-ie8": "set $NODE_PATH=. && node-sass app/assets/sass/application-ie8.scss --output public/stylesheets --quiet --output-style expanded --source-map true --include-path node_modules/govuk_frontend",
    "test": "npm run test-unit -- $@ && npm run test-accessibility -- $@ && npm run test-component -- $@",
    "test-unit": "set $NODE_PATH=. && set $LOG_LEVEL=error && mocha test/unit --reporter spec --recursive --timeout 15000",
    "test-component": "set $NODE_PATH=. && set $LOG_LEVEL=error && mocha test/component --reporter spec --recursive --timeout 15000",
    "test:a11y": "set $NODE_PATH=. && set $LOG_LEVEL=error && set $ENABLE_TRACKING=false && mocha test/accessibility --timeout 15000",
    "test-accessibility": "set $NODE_PATH=. && set $LOG_LEVEL=error && set $ENABLE_TRACKING=false && mocha test/accessibility --timeout 15000",
    "test-e2e": "set $NODE_PATH=. && node ./node_modules/codeceptjs/bin/codecept.js run -c ./test/end-to-end/ --steps --reporter mochawesome",
    "test:crossbrowser_IE11": "set $NODE_PATH=. && codeceptjs run-multiple microsoftIE11 -c test/end-to-end/saucelabs.conf.js --plugins retryFailedStep --steps --reporter mocha-multi",
    "test:crossbrowser_Edge": "set $NODE_PATH=. && codeceptjs run-multiple microsoftEdge -c test/end-to-end/saucelabs.conf.js --plugins retryFailedStep --steps --reporter mocha-multi",
    "test:crossbrowser_chrome": "set $NODE_PATH=. && codeceptjs run-multiple chrome -c test/end-to-end/saucelabs.conf.js --plugins retryFailedStep --steps --reporter mocha-multi",
    "test:crossbrowser_firefox": "set $NODE_PATH=. && codeceptjs run-multiple firefox -c test/end-to-end/saucelabs.conf.js --plugins retryFailedStep --steps --reporter mocha-multi",
    "test:crossbrowser": "./bin/run-crossbrowser-tests.sh",
    "test:smoke": "mocha test/smoke/healthcheck.js --timeout 25000 --reporter spec",
    "test-contract": "set $NODE_PATH=. && set $LOG_LEVEL=error && mocha 'test/contract/addresslookup/*.js' 'test/contract/generatepin/*.js' --timeout 15000 --reporter spec",
    "test-pact": "set $NODE_PATH=. && set $LOG_LEVEL=error && mocha 'test/contract/pact/*.js' --timeout 15000 --reporter spec",
    "publish-pact": "set $NODE_PATH=. && set $LOG_LEVEL=error && mocha 'test/contract/publish/*.js' --timeout 15000 --reporter spec",
    "lint": "set $NODE_PATH=. && eslint .",
    "eslint": "set $NODE_PATH=. && eslint .",
    "test:nsp": "set $NODE_PATH=. && nsp check",
    "security": "set $NODE_PATH=. && nsp check",
    "test:functional": "npm run test-contract && ./bin/run-functional-tests.sh",
    "test:codecov-upload": "set $NODE_ENV=test && set $NODE_PATH=. && istanbul cover _mocha './test/**/test*.js' -- -R spec && codecov",
    "test:coverage": "set $NODE_ENV=test && set $NODE_PATH=. && istanbul cover _mocha './test/unit/test*.js' './test/component/**/test*.js' -- --timeout 5000",
    "test-coverage": "set $NODE_ENV=test && set $NODE_PATH=. && istanbul cover _mocha './test/**/test*.js'",
    "sonar-scanner": "node_modules/sonar-scanner/bin/sonar-scanner",
    "sonar-scan": "node_modules/sonar-scanner/bin/sonar-scanner -Dsonar.projectName='Probate :: frontend' -Dsonar.sources=app -Dsonar.language=js -Dsonar.cpd.exclusions=app/steps/ui/tasklist/includes/**,app/steps/ui/executors/roles/** -Dsonar.exclusions=app/steps/ui/tasklist/includes/** -Dsonar.exclusions=app/** -Dsonar.javascript.lcov.reportPaths=coverage/lcov.info",
    "git-info": "node-git-info-json",
    "//": "The below scripts are just stubs, don't use them in production",
    "s2s-stub": "set $NODE_PATH=. && node test/service-stubs/idam.js",
    "fees-api-stub": "set $NODE_PATH=. && node test/service-stubs/fees-api.js",
    "submit-stub": "set $NODE_PATH=. && node test/service-stubs/submit.js",
    "persistence-stub": "set $NODE_PATH=. && node test/service-stubs/persistence.js",
    "business-stub": "set $NODE_PATH=. && node test/service-stubs/business.js",
    "payment-stub": "set $NODE_PATH=. && node test/service-stubs/payment.js",
    "find-address-stub": "set $NODE_PATH=. && node test/service-stubs/find-address.js",
    "feature-toggles-stub": "set $NODE_PATH=. && node test/service-stubs/feature-toggles.js",
    "test:fullfunctional": "set $NODE_PATH=. && node ./node_modules/codeceptjs/bin/codecept.js run-multiple parallel -c ./test/end-to-end/ --steps --reporter mochawesome"
  },
  "dependencies": {
    "@hmcts/ctsc-web-chat": "^0.3.5",
    "@hmcts/nodejs-logging": "^3.0.0",
    "@hmcts/os-places-client": "^1.0.4",
    "ajv": "^4.11.8",
    "applicationinsights": "^1.4.0",
    "basic-auth": "^2.0.1",
    "body-parser": "^1.19.0",
    "cheerio": "^1.0.0-rc.3",
    "co": "^4.6.0",
    "codacy-coverage": "^3.4.0",
    "codecov": "^4.0.0-0",
    "connect-redis": "^3.4.2",
    "connect-timeout": "^1.9.0",
    "cookie-parser": "^1.4.4",
    "csurf": "^1.10.0",
    "debug": "^4.1.1",
    "email-validator": "^2.0.4",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "file-type": "^12.1.0",
    "git-rev-sync": "^1.12.0",
    "govuk-frontend": "^3.0.0",
    "helmet": "^3.20.0",
    "https-proxy-agent": "^2.2.2",
    "i18next": "^17.0.8",
    "ioredis": "^4.14.0",
    "jsdom": "^15.1.1",
    "lodash": "^4.17.15",
    "mocha-lcov-reporter": "^1.3.0",
    "moment": "^2.24.0",
    "multer": "^1.4.2",
    "node-emoji": "^1.10.0",
    "node-fetch": "^1.7.3",
    "node-git-info-json": "^0.1.1",
    "node-sass": "^4.12.0",
    "numeral": "^2.0.6",
    "nunjucks": "^3.2.0",
    "otp": "^0.1.3",
    "randomstring": "^1.1.5",
    "readdir": "^0.1.0",
    "request": "^2.88.0",
    "require-directory": "^2.1.1",
    "sanitizer": "^0.1.3",
    "serve-favicon": "^2.5.0",
    "simple-random": "^1.0.3",
    "superagent": "^5.1.0",
    "traverse": "^0.6.6",
    "tunnel-agent": "^0.6.0",
    "uuid": "^3.3.2",
    "validator": "^11.1.0"
  },
  "devDependencies": {
    "@pact-foundation/pact": "^7.2.0",
    "@pact-foundation/pact-node": "^6.20.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.3.0",
    "chai-string": "^1.5.0",
    "codeceptjs": "^2.2.1",
    "eslint": "6.1.0",
    "eslint-plugin-mocha": "^6.0.0",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^3.5.3",
    "mocha-jenkins-reporter": "^0.4.2",
    "mocha-multi": "^1.1.0",
    "mochawesome": "^4.0.1",
    "nock": "^10.0.6",
    "nodemon": "^1.19.1",
    "nsp": "^3.2.1",
    "pa11y": "^4.13.2",
    "phantomjs-prebuilt": "^2.1.16",
    "proxyquire": "^2.1.1",
    "puppeteer": "^1.19.0",
    "rewire": "^4.0.1",
    "selenium-standalone": "^6.16.0",
    "sinon": "^1.17.7",
    "sinon-chai": "^3.3.0",
    "sonar-scanner": "^3.1.0no",
    "sonarqube-scanner": "^2.5.0",
    "superagent-proxy": "^2.0.0",
    "supertest": "^4.0.2",
    "unirest": "^0.6.0",
    "uuid": "^3.3.2",
    "watch": "^1.0.2",
    "webdriverio": "^5.11.12",
    "when": "^3.7.8"
  }
}
